<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="fr.nelaupe.benchmark" >

    <application
        android:name=".BenchmarkApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:largeHeap="true"
        android:supportsRtl="true"
        android:theme="@style/AppTheme" >
        <activity android:name="fr.nelaupe.benchmark.MainActivity" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

    <meta-data android:name="AA_DB_NAME" android:value="active.db" />
    <meta-data android:name="AA_DB_VERSION" android:value="1" />
    <meta-data android:name="AA_MODELS" android:value="fr.nelaupe.benchmark.active.ActiveAndroidPerson" />

    <meta-data android:name="DATABASE" android:value="sugar_example.db" />
    <meta-data android:name="VERSION" android:value="2" />
    <meta-data android:name="QUERY_LOG" android:value="false" />
    <meta-data android:name="DOMAIN_PACKAGE_NAME" android:value="fr.nelaupe.benchmark.sugar" />

</manifest>